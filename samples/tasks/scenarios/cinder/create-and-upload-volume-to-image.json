            "args": {
                "size": 1,
                "force": false,
                "container_format": "bare",
                "disk_format": "raw",
                "do_delete": true,
                "image": {
                    "name": "^cirros.*uec$"
                }
            },
            "runner": {
                "type": "constant",
                "times": 3,
                "concurrency": 2
